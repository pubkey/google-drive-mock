{
  "name": "google-drive-mock",
  "version": "1.1.1",
  "description": "Mock-Server that simulates being google-drive. Used for testing.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "TEST_TARGET=mock vitest run",
    "test:slow": "LATENCY=20 vitest run",
    "test:browser": "start-server-and-test dev http://localhost:3000 'TEST_TARGET=mock BROWSER_ENABLED=true vitest run --browser --no-file-parallelism'",
    "test:browser:real": "TEST_TARGET=real BROWSER_ENABLED=true vitest run --browser",
    "test:real": "ts-node scripts/check-token.ts && TEST_TARGET=real vitest run",
    "example:login": "ts-node examples/serve-login.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pubkey/google-drive-mock.git"
  },
  "keywords": [
    "google-drive",
    "mock",
    "testing",
    "server",
    "api",
    "simulator",
    "rxdb"
  ],
  "author": "pubkey",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/pubkey/google-drive-mock/issues"
  },
  "homepage": "https://github.com/pubkey/google-drive-mock#readme",
  "peerDependencies": {
    "express": "5.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/node": "25.1.0",
    "@types/supertest": "6.0.3",
    "@vitest/browser": "4.0.18",
    "@vitest/browser-playwright": "4.0.18",
    "async-test-util": "2.5.0",
    "cors": "2.8.6",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "express": "5.2.1",
    "globals": "17.2.0",
    "playwright": "1.58.0",
    "start-server-and-test": "2.1.3",
    "supertest": "7.2.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18"
  }
}
